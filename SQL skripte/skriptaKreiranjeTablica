CREATE SEQUENCE evidencijaDjelatnikaBanke_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE evidencijaOsoba_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE klijent_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE klijentEvidencijaOsoba_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE korisnikUsluge_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE racun_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE racunKreiraniIzvod_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE racunUgovoreniIzvod_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikDrzava_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikFormataIzvoda_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikIdentifikacijskihIsprava_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikKanalaIzvoda_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikPeriodikaIzvoda_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikVrsteOdnosa_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE sifarnikVrsteUsluga_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE SEQUENCE usluga_SEQ START WITH 1 INCREMENT BY 1 MINVALUE 1 CACHE 20;
CREATE TABLE cjenikIzvoda (
  sifraFormataIzvoda number(10) NOT NULL, 
  sifraKanalaIzvoda  number(10) NOT NULL, 
  cijena             float(10) NOT NULL, 
  djelatnikBankeID   number(10), 
  datumPromjene      date NOT NULL, 
  PRIMARY KEY (sifraFormataIzvoda, 
  sifraKanalaIzvoda));
CREATE TABLE evidencijaDjelatnikaBanke (
  djelatnikBankeID    number(10) NOT NULL, 
  osobaID             number(10) NOT NULL, 
  radnoMjesto         varchar2(100) NOT NULL, 
  korisnickoIme       varchar2(100) NOT NULL UNIQUE, 
  lozinka             varchar2(100) NOT NULL, 
  djelatnikBankeID_FK number(10), 
  datumPromjene       date NOT NULL, 
  PRIMARY KEY (djelatnikBankeID));
CREATE TABLE evidencijaOsoba (
  osobaID                      number(10) NOT NULL, 
  OIB                          varchar2(11) NOT NULL UNIQUE, 
  ime                          varchar2(50) NOT NULL, 
  prezime                      varchar2(50) NOT NULL, 
  ulica                        varchar2(255) NOT NULL, 
  kbr                          varchar2(5) NOT NULL, 
  brPoste                      number(5) NOT NULL, 
  telefon                      varchar2(255), 
  mobitel                      varchar2(255), 
  email                        varchar2(100), 
  sifraIdentifikacijskeIsprave number(10) NOT NULL, 
  brojIdentifikacijskeIsprave  varchar2(50) NOT NULL, 
  djelatnikBankeID             number(10), 
  datumPromjene                date NOT NULL, 
  PRIMARY KEY (osobaID));
CREATE TABLE izvodValute (
  kreiraniIzvodID number(10) NOT NULL, 
  sifraValute     varchar2(3) NOT NULL, 
  PRIMARY KEY (kreiraniIzvodID, 
  sifraValute));
CREATE TABLE klijent (
  klijentID        number(10) NOT NULL, 
  OIB              varchar2(11) NOT NULL UNIQUE, 
  MBS              varchar2(8) NOT NULL UNIQUE, 
  naziv            varchar2(255) NOT NULL, 
  ulica            varchar2(255) NOT NULL, 
  kbr              varchar2(5) NOT NULL, 
  brPoste          number(5) NOT NULL, 
  sifraDrzave      number(10) NOT NULL, 
  sifraSektora     varchar2(2) NOT NULL, 
  telefon          varchar2(50), 
  mobitel          varchar2(50), 
  email            varchar2(100), 
  sifraAktivnosti  number(1) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (klijentID));
COMMENT ON COLUMN klijent.MBS IS 'Matični broj subjekta
- obavezan za unos kad je sektor = 41 (obrtnici)';
COMMENT ON COLUMN klijent.datumPromjene IS 'dohvaćanje preko SYSDATE from dual';
CREATE TABLE klijentEvidencijaOsoba (
  klijentOsobaID   number(10) NOT NULL, 
  klijentID        number(10) NOT NULL, 
  osobaID          number(10) NOT NULL, 
  sifraVrsteOdnosa number(10) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (klijentOsobaID));
COMMENT ON TABLE klijentEvidencijaOsoba IS 'Ovlaštene osobe od strane klijenta';
CREATE TABLE korisnikUsluge (
  korisnikUslugeID number(10) NOT NULL, 
  uslugaID         number(10) NOT NULL, 
  klijentOsobaID   number(10) NOT NULL, 
  datumOtvaranja   date NOT NULL, 
  datumZatvaranja  date, 
  sifraAktivnosti  number(1) NOT NULL, 
  korisnickoIme    varchar2(100) NOT NULL, 
  lozinka          varchar2(100) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (korisnikUslugeID));
COMMENT ON TABLE korisnikUsluge IS 'Veza između konkretne usluge koju je klijent ugovorio i osoba koje mogu koristiti tu uslugu';
CREATE TABLE kreiraniIzvodStavke (
  kreiraniIzvodID           number(10) NOT NULL, 
  stavkaID                  number(10) NOT NULL, 
  datumVrijemePromjeneSalda timestamp(0) NOT NULL, 
  sifraValute               varchar2(3) NOT NULL, 
  datumValute               date NOT NULL, 
  opis                      varchar2(255) NOT NULL, 
  uplata                    float(10), 
  isplata                   float(10), 
  PRIMARY KEY (kreiraniIzvodID, 
  stavkaID));
CREATE TABLE racun (
  racunID              number(10) NOT NULL, 
  IBAN                 varchar2(21) NOT NULL UNIQUE, 
  klijentID            number(10) NOT NULL, 
  sifraVrsteRacuna     varchar2(10) NOT NULL, 
  sifraAktivnosti      number(1) NOT NULL, 
  datumOtvaranja       date NOT NULL, 
  datumZatvaranja      date, 
  blokiran             number(1) NOT NULL CHECK(blokiran IN (0, 1)), 
  sifraPeriodikeIzvoda number(10) NOT NULL, 
  upitUStanje          number(1) NOT NULL CHECK(upitUStanje IN (0, 1)), 
  obracunNaknade       number(1) NOT NULL CHECK(obracunNaknade IN (0, 1)), 
  djelatnikBankeID     number(10), 
  datumPromjene        date NOT NULL, 
  PRIMARY KEY (racunID));
COMMENT ON COLUMN racun.blokiran IS '0 - neblokiran
1 - blokiran';
COMMENT ON COLUMN racun.upitUStanje IS '0 - NE - klijent ne dozvoljava da mu se radi upit u stanje s nekih aplikacija
1 - DA - klijent dozvoljava da mu se radi upit u stanje s nekih aplikacija';
COMMENT ON COLUMN racun.obracunNaknade IS '0 - NE - ne obračunava se naknada za usluge
1 - DA -  obračunava se naknada za usluge';
CREATE TABLE racunKreiraniIzvod (
  kreiraniIzvodID       number(10) NOT NULL, 
  racunUgovoreniIzvodID number(10) NOT NULL, 
  datumIzvoda           date NOT NULL, 
  brojIzvoda            number(5) NOT NULL, 
  prethodnoStanje       float(10) NOT NULL, 
  zavrsnoStanje         float(10) NOT NULL, 
  datumVrijemeKreiranja timestamp(0) NOT NULL, 
  PRIMARY KEY (kreiraniIzvodID));
CREATE TABLE racunUgovoreniIzvod (
  racunUgovoreniIzvodID  number(10) NOT NULL, 
  racunID                number(10) NOT NULL, 
  sifraFormataIzvoda     number(10) NOT NULL, 
  sifraKanalaIzvoda      number(10) NOT NULL, 
  datumVrijemeUgovaranja timestamp(0) NOT NULL, 
  PRIMARY KEY (racunUgovoreniIzvodID));
CREATE TABLE sifarnikAktivnosti (
  sifraAktivnosti  number(1) NOT NULL, 
  nazivAktivnosti  varchar2(50) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraAktivnosti));
CREATE TABLE sifarnikDrzava (
  sifraDrzave      number(10) NOT NULL, 
  nazivDrzave      varchar2(100) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraDrzave));
CREATE TABLE sifarnikFormataIzvoda (
  sifraFormataIzvoda number(10) NOT NULL, 
  nazivFormata       varchar2(50) NOT NULL, 
  djelatnikBankeID   number(10), 
  datumPromjene      date NOT NULL, 
  PRIMARY KEY (sifraFormataIzvoda));
CREATE TABLE sifarnikIdentifikacijskihIsprava (
  sifraIdentifikacijskeIsprave number(10) NOT NULL, 
  nazivIdentifikacijskeIsprave varchar2(100) NOT NULL, 
  djelatnikBankeID             number(10), 
  datumPromjene                date NOT NULL, 
  PRIMARY KEY (sifraIdentifikacijskeIsprave));
CREATE TABLE sifarnikKanalaIzvoda (
  sifraKanalaIzvoda number(10) NOT NULL, 
  nazivKanala       varchar2(50) NOT NULL, 
  djelatnikBankeID  number(10), 
  datumPromjene     date NOT NULL, 
  PRIMARY KEY (sifraKanalaIzvoda));
CREATE TABLE sifarnikMjesta (
  brPoste          number(5) NOT NULL, 
  mjesto           varchar2(255) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (brPoste));
CREATE TABLE sifarnikPeriodikaIzvoda (
  sifraPeriodikeIzvoda number(10) NOT NULL, 
  nazivPeriodike       varchar2(50) NOT NULL, 
  djelatnikBankeID     number(10), 
  datumPromjene        date NOT NULL, 
  PRIMARY KEY (sifraPeriodikeIzvoda));
CREATE TABLE sifarnikSektora (
  sifraSektora     varchar2(2) NOT NULL, 
  nazivSektora     varchar2(255) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraSektora));
CREATE TABLE sifarnikTipovaRacuna (
  sifraTipaRacuna  varchar2(2) NOT NULL, 
  nazivTipaRacuna  varchar2(100) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraTipaRacuna));
CREATE TABLE sifarnikValuta (
  sifraValute      varchar2(3) NOT NULL, 
  valuta           varchar2(3) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraValute));
CREATE TABLE sifarnikVrsteBankovnihRacuna (
  sifraVrsteRacuna varchar2(10) NOT NULL, 
  nazivVrsteRacuna varchar2(100) NOT NULL, 
  sifraTipaRacuna  varchar2(2) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraVrsteRacuna));
CREATE TABLE sifarnikVrsteOdnosa (
  sifraVrsteOdnosa number(10) NOT NULL, 
  nazivVrsteOdnosa varchar2(100) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraVrsteOdnosa));
CREATE TABLE sifarnikVrsteUsluga (
  sifraVrsteUsluge number(10) NOT NULL, 
  nazivVrsteUsluge varchar2(100) NOT NULL, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (sifraVrsteUsluge));
CREATE TABLE usluga (
  uslugaID         number(10) NOT NULL, 
  klijentID        number(10) NOT NULL, 
  racunID          number(10), 
  sifraVrsteUsluge number(10) NOT NULL, 
  sifraAktivnosti  number(1) NOT NULL, 
  datumOtvaranja   date NOT NULL, 
  datumZatvaranja  date, 
  djelatnikBankeID number(10), 
  datumPromjene    date NOT NULL, 
  PRIMARY KEY (uslugaID));
COMMENT ON COLUMN usluga.racunID IS 'Može a i ne mora biti upisan, ako je upisan znači da ta usluga vrijedi samo za taj konkretni račun.';
